{
    "version": 2,
    "builds": [
        {
            "src": "project/index.js",
            "use": "@vercel/node"
        }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "project/index.js"
      }
    ],
    "functions": {
    "api/**/*.js": {
      "timeout": 30
    }
  }
  }
  